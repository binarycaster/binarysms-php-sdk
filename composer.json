{
  "name": "binarycaster/binarysms",
  "description": "BinarySMS is a package for integrate sms gateway of Binary SMS",
  "license": "MIT",
  "keywords": [
    "BinaryCaster",
    "BinarySMS"
  ],
  "authors": [
    {
      "name": "Akbar Hossain",
      "email": "akbarhossain15@gmail.com",
      "homepage": "https://akbarhossainr.github.io",
      "role": "Developer"
    }
